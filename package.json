{
    "name": "mean",
    "description": "MEAN - A Modern Stack: MongoDB, ExpressJS, AngularJS, NodeJS. (BONUS: Passport User Support).",
    "version": "1.0.0",
    "private": false,
    "author": "Amos Haviv",
    "repository": {
        "type": "git",
        "url": "https://github.com/linnovate/mean.git"
    },
    "engines": {
        "node": "0.10.x",
        "npm": "1.2.x"
    },
    "scripts": {
        "start": "node node_modules/grunt-cli/bin/grunt",
        "test": "node node_modules/grunt-cli/bin/grunt test",
        "postinstall": "bower install",
        "pm2-start": "./node_modules/pm2/bin/pm2 start server.js -i max",
        "pm2-stop": "./node_modules/pm2/bin/pm2 stop server.js",
        "pm2-delete": "./node_modules/pm2/bin/pm2 delete server.js",
        "pm2-restart": "./node_modules/pm2/bin/pm2 restart server.js",
        "pm2-monit": "./node_modules/pm2/bin/pm2 monit",
        "pm2-logs": "./node_modules/pm2/bin/pm2 logs",
        "pm2-reload": "./node_modules/pm2/bin/pm2 reload all"
    },
    "dependencies": {
        "express": "~3.9.x",
        "method-override": "2.2.0",
        "jade": "1.7.0",
        "mongoose": "3.8.17",
        "connect-mongo": "0.4.1",
        "connect-flash": "0.1.1",
        "passport": "0.2.1",
        "passport-local": "1.0.0",
        "underscore": "1.7.0",
        "request": "2.45.0",
        "csv": "0.3.7",
        "async": "0.2.10",
        "view-helpers": "0.1.5",
        "mean-logger": "0.0.1",
        "forever": "0.11.1",
        "bower": "1.3.12",
        "grunt": "0.4.5",
        "grunt-cli": "0.1.13",
        "grunt-env": "0.4.2",
        "grunt-bower-task": "0.4.0",
        "sylvester": "0.0.21",
        "numjs": "0.0.3",
        "pm2": "0.11.0",
        "agenda": "0.6.19"
    },
    "devDependencies": {
        "supertest": "0.14.0",
        "should": "4.0.4",
        "grunt-contrib-watch": "0.6.1",
        "grunt-contrib-jshint": "0.10.0",
        "grunt-nodemon": "0.3.0",
        "grunt-concurrent": "1.0.0",
        "grunt-mocha-test": "0.12.1"
    }
}
